<script type="text/javascript">
    $(document).ready(function () {
        var datas = JSON.parse(storage.getItem('police-info'));
        //console.log(datas);
        var poli_id_my = datas.police.poliId;
        var stat_id = datas.police.statId;
        var role = datas.police.poliRole;
        var stat_name = datas.police.statName;

        if(role==01){
            getStatisByPoli(poli_id_my);

            $('#menu-comm').click(function() {
                createtree10(poli_id_my);
        });
        
        }
        else if(role==10){
            getStatisByStat(stat_id);

            $('#menu-comm').click(function() {
                createtree20(stat_name,stat_id);
            });
        }

        admin();
        

        mult('#bread-zhuzhai','#zhuzhai-all','住宅');
        mult('#bread-teye','#teye-all','特业');
        mult('#bread-school','#school-all','学校');
        mult('#bread-place','#place-all','场所');
        mult('#bread-others','#others-all','其他');
        mult('#bread-import','#import-all','重点关注');

    });
</script>
