/* CSS Document */

*{
	margin:0px;padding:0px;
}
p{
	color:rgb(80,100,120);
	font-size:36px;
	padding-left:100px;
	padding-top:30px;
}
#wrapper{
	width:100%;
	height:610px;
	background:url("images/login-back.jpg") no-repeat top;
	background-size:100%;
	position:absolute;
}
#loginbar{
	width:350px;
	height:220px;
	margin-left:35%;
	margin-top:135px;
	border-radius:5px;
	padding-left:35px;
	padding-top:30px;
	border:solid 2px rgb(13,35,76);
	background:url("images/login-log.png") no-repeat top;
	background-size:100% 62px;
}
#loginbar ul{
	list-style-type:none;
	padding-top:50px;
}
#loginbar ul li{
	padding:5px;
}
#uname,#upassword{
	height:28px;
	width:220px;
	border-radius:2px;
	border:solid 1px rgb(153,153,153); 
}
span{
	color:rgb(70,70,70);
	font-family:HeiSim;
}
#remember{
	margin-left:47px;
	font-size:12px;
	color:rgb(102,102,102);
}
#submit,#reset{
	margin-left:47px;
	width:90px;
	height:30px;
	color:#FFF;
	background:rgb(70,80,100);
	border:solid 1px rgb(70,80,100);
	border-radius:4px;
}
#reset{
margin-left:35px;
}