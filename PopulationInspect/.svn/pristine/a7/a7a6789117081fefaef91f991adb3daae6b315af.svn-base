function checkdata(){
	if((form.uname.value==form.upassword.value)&&(form.uname.value)!=""){
		alert("账号和密码不能相同！");
		return false;
	}
	if((form.uname.value=="")||(form.upassword.value=="")){
		alert("账号或密码不能为空！");
		return false;
	}
	var parten =/(^\s*)|(\s*$)/g; 
    if(parten.test(form.upassword.value)){
	   alert("密码不能含有空格");
	 }
}