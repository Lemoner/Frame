/*边框灰色：#d2d6d9*/
* {
    font-family: 微软雅黑;
    margin: 0;
    padding: 0;
}
.green{
    color: #5cb85c;
}
.red{
    color: #d9534f;
}
.blue{
    color: #5bc0de;
}
.yellow{
    color: #f7e600;
}
.num{
    font-family: Georgia;
}
html,body{
    height: 100%;
    width: 100%;
    min-width: 1180px;
}
i{
    font-style: normal;
}
/*navbar min-height*/
.navbar-nav>li>a {
    padding-top: 10px;
    padding-bottom: 10px;
}
.navbar-brand {
    height: 40px;
    padding-top: 10px;
}
.navbar {
    min-height: 40px;
}


.navbar-toggle {
    margin-top: 4px;
    margin-bottom: 4px;
}
/*navbar min-height*/

p.navbar-icon {
    font-size: 20px;
    line-height: 60px;
    display: inline-block;
    height: 60px;
    text-align: center;
    margin: 0 0 0 0;
    float: right;
}

a.navbar-link {
    line-height: 62px;
    display: inline-block;
    height: 62px;
    width: 60px;
    margin-left: -7px;
    border-left: 1px solid #999;
}
a.navbar-link:hover{
    background-color: #5bc0de;
}

label {
    font-weight: normal;
    margin-bottom: 0;
    border: none;
}

.navbar-header-my {
    margin-left: 10px;
}

.navbar-my {
    min-width: 1180px;
    min-height: 63px;
    height: 6%;
    margin: 0;
    border: none;
    border-radius: 0;
    background: url(../images/LOGO栏.png);
    background-size: cover;
}
.logo-img{
    margin-top: 3px;
    width: 50px;
    height: 50px;
    float: left;
}
.navbar-my .logo {
    font-family: 黑体;
    font-size: 24px;
    font-weight: bold;
    float: left;
    margin-top: 20px;
    color: white;
    text-shadow: #ccc;
}

.logout{
    width: 60%;
    min-width: 780px;
    min-height: 63px;
    position: absolute;
    right: 0;
    top: 0;
}

.navbar-my .logout span {
    padding-right: 10px;
    padding-left: 10px;
    font-weight: normal;
    color: white;
}

.navbar-my .logout a:hover {
    text-decoration: none;
    color: white;
}

.admin {
    margin-top: 10px;
    width: 520px;
    font-size: 10px;
    color: white;
}
.admin img{
    width: 40px;
    height: 40px;
    margin-right: 20px;
    float: left;
}
ul.admin-info,ul.admin-web{
    margin-bottom: 0;
}
.admin span{
    height: 17px;
    line-height: 17px;
}
.wraper{
    height: 100%;
    /*padding-top: 63px;*/
}
.main{
    top: 63px;
    min-width: 1180px;
    height: 90%;
    width: 100%;
}
.left {
    height: 100%;
    overflow-y: scroll;
    padding: 0;
    border-right: double #d2d6d9;
    background-color: #e8edf3;
}

.ac-container .grid {
    position: relative;
    z-index: 20;
    display: block;
    padding: 10px 20px;
    cursor: pointer;
    color: #818a93;
    background: inherit;
}

.ac-container .grid i {
    font-size: 14px;
    line-height: 14px;
    display: inline-block;
    height: 14px;
}

.ac-container .grid0,
.ac-container .grid1,
.ac-container .grid2 {
    border-bottom: 1px #d0dad3 solid;
}

.ac-container input {
    display: none;
}

.ac-container article {
    position: relative;
    z-index: 10;
    overflow-x: hidden;
    overflow-y: scroll;
    height: 0;
    margin-top: -1px;
    background-color: #ffffff;
}

.ac-container input:checked ~ article.ac-small-1 {
    overflow: auto;
    height: 250px;
    /*height: auto;*/
    /*max-height: 250px;*/
}

.ac-container input:checked ~ article.ac-small-2 {
    overflow: auto;
    height: auto;
    background: #e8edf3;
    /*max-height: 300px;*/
}

.ac-container label.grid1:after,.ac-container label.grid2:after {
    position: absolute;
    top: 12px;
    right: 13px;
    width: 24px;
    height: 24px;
    content: '';
    background: url('../images/arrow-r.png') no-repeat 0 0;
}

.ac-container input:checked + label.grid1:after ,.ac-container input:checked + label.grid2:after {
    background: url(../images/arrow-d.png) no-repeat 0 0;
}

.ac-small ul {
    width: 100%;
    padding: 0 0 0 1em;
}

.ac-small ul li {
    margin: 0 0 .5em;
    list-style-type: none;
}

.ac-small ul li a {
    font-size: 10px;
    text-decoration: none;
    color: #000;
}

.grid i {
    width: 24px;
    height: 24px;
    padding-right: 2em;
    padding-bottom: .3em;
}

/*辖区建筑菜单部分*/

/*下拉菜单*/
ul.subfolder,ul.file{
    display: none;
}

.tree {
    width: 100%;
    margin-top: 10px;
}


.tree li {
    font-size: 10px;
    position: relative;
    padding-left: 16px;
    list-style: none;
    background: url(../images/folder.gif) 0 2px no-repeat;
}

.tree li.file a {
    display: block;
    text-decoration: none;
}

.info .info-t {
    font-size: 14px;
    line-height: 30px;
    height: 30px;
    border-bottom: 1px solid #d2d6d9;
}

.info ul li {
    font-size: 13px;
    /*line-height: 20px;*/
    /*height: 20px;*/
    padding-left: 5px;
}

.charts {
    width: 100%;
    height: auto;
    min-height: 250px;
    margin: 0 auto;
}

.right {
    padding: 0;
    height: 100%;
}
.right-top{
    height: 7%;
    min-height: 40px;
    background-color: #f5f5f5;
}
.right-top-t{
    height: 35px;
}
.right-top-t ul{
    margin: 5px 0 0 0;
}
.right-top-t ul li {
    display: inline-block;
    margin-left: 5px;
}
.charts-list {
    position: absolute;
    z-index: -1;
    top: 0;
    display: none;
    width: 100%;
    height: 250px;
    border-bottom: 2px solid #448ac6;
    background-color: #536073;
}

.hide-btn {
    font-size: 10px;
    line-height: 20px;
    position: absolute;
    z-index: 5;
    top: 0;
    right: 0;
    width: 70px;
    height: 20px;
    cursor: pointer;
    text-align: center;
    color: #fff;
    background-color: #d2d6d9;
}

.hide-btn2 {
    font-size: 10px;
    position: absolute;
    z-index: 5;
    top: 40px;
    right: 0;
    width: 20px;
    height: 20px;
    cursor: pointer;
    text-align: center;
    color: #fff;
    background-color: #d2d6d9;
}

.map {
    /*width: 66.6%;*/
    height: 93%;
    padding: 0;
    border: 1px solid #d2d6d9;
}

.bread {
    height: 40px;
    margin: 0;
    border-radius: 0;
}
.breadcrumb > li + li:before {
    color: #CCCCCC;
    content: "> ";
    padding: 0 5px;
}
.breadcrumb {
    padding-left: 5px;
    margin: 4px 0 0 0;
}

.tab {
    height: 93%;
    padding-top: 5px;
    padding-left: 1%;
    background-color: white;
    display: none;
    border-top: 1px solid #d2d6d9;
    overflow-y: scroll;
}

.tab-my a.tab-d {
    font-family: 黑体;
    line-height: 30px;
    display: block;
    height: 30px;
    padding: 0 10px;
    text-align: center;
    border: 1px solid #d2d6d9;
    border-radius: 0;
}

a.tab-d:link {
    display: block;
    text-decoration: none;
    background-color: #cdcdcd;
}
.nav-tabs>li.active>a,
.nav-tabs>li.active>a:focus,
.nav-tabs>li.active>a:hover {
    color: #777777;
    background-color: #f7f700;
}
a.tab-d:hover {
    color: #777777;
    border-bottom: 2px solid #34f7b7;
}

.btn-my {
    width: 35px;
    height: 35px;
    margin-top: 5px;
    margin-bottom: 5px;
    border-radius: 50%;
}
.pager li>a, .pager li>span {
    padding: 0px 0px;
    border-radius: 50%;
}
.pager li>a img {
    width: 30px;
    height: 30px;
}
.floor {
    min-height: 50px;
    border-bottom: 1px solid #d2d6d9;
}
.floor-change{
    height: 60px;
    margin-top: 5px;
}
a.floor-l,a.floor-n{
    padding: 0 0 0 0;
    display: inline-block;
    width: 50px;
    height: 50px;
}

.floor-num {
    display: inline-block;
    width: 70%;
    text-align: center;
}
.badge-my{
    display: inline-block;
    min-width: 10px;
    padding: 3px 7px;
    font-size: 12px;
    font-weight: 700;
    line-height: 1;
    color: #fff;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    background-color: #333;
    border-radius: 10px;
}
.badge-success{
    color: #5cb85c;
    background-color: #fff;
}

.btn-default .badge-success{
    border: 1px solid #5cb85c;
}

.badge-danger{
    color: #d9534f;
    background-color: #fff;
}
.btn-default .badge-danger{
    border: 1px solid #d9534f;
}
.house-info{
    display: none;
    padding-top: 10px;
    height: auto;
    overflow: auto;
    text-align: left;
    border-top: 1px solid #d2d6d9;
}
.house-info ul{
    list-style-type: none;
}
.house-info .modal{
    margin-top: 12%;
}
.btn-h {
    width: 11%;
    height: 30px;
    margin: 3% 4.5%;
}
.btn-h:focus {
    border: 2px dashed #fff;
}
.btn-h.btn-default:focus {
    border: 2px dashed #000;
}

.btn-h .badge-my {
    position: absolute;
    top: -12px;
    left: 0;
    width: 100%;
    padding-right: 0;
    padding-left: 0;
}

.people-addr-h {
    height: auto;
    max-height: 280px;
    overflow-y: scroll;
    padding-top: 10px;
}

.people-addr-h p {
    display: inline-block;
    width: 100%;
    padding-top: 10px;
}

.people-addr-h p button {
    margin-bottom: 20px;
}

.people-info {
    display: none;
    margin-top: 10px;
}
.media-body span{
    padding: 0 0 0 0;
}
.people-info-sub {
    display: inline-block;
    height: auto;
    padding-top: 10px;
    padding-bottom: 10px;
    border-top: 1px dotted #d2d6d9;
    border-bottom: 1px dashed #d2d6d9;
}

.sex {
    font-size: 14px;
    line-height: 22px;
}

.tel {
    font-size: 16px;
    color: darkred;
}

.id-num {
