function createtree10(poliId){
    var treebox = $('#tree');
    var breadcrumb = $("#breadcrumb");
    breadcrumb.html("");
    treebox.html('');
    var p = '<ul class="folder">';
    p += '<li class="building0" id="station" ><a class="m0" href="javascript:void(0)" onclick="treeLevel10('+poliId+');getStatisByPoli('+poliId+')"> 所管辖区</a><ul class="subfolder" id="menu1-ul"></li>';
    p += '</ul>';

    treebox.prepend(p);
// 面包屑导航 添加辖区
    var ol = '<ol class="breadcrumb" id="breadcrumb1">';
    ol +='<li id="root1"><a  href="javascript:void(0)">所管辖区</a></li>';
    breadcrumb.append(ol);
}

function createtree20(statName,statId){
    var treebox = $('#tree');
    var breadcrumb = $("#breadcrumb");
    breadcrumb.html("");
    treebox.html('');
    var p = '<ul class="folder">';
    p += '<li class="building0" id="station" ><a class="m0" href="javascript:void(0)" onclick="treeLevel20('+statId+');getStatisByStat('+statId+')">'+statName+'</a><ul class="subfolder" id="menu1-ul"></li>';
    p += '</ul>';
    //标头添加公安局
    treebox.prepend(p);
    var ol = '<ol class="breadcrumb" id="breadcrumb1">';
    ol +='<li id="root1"><a  href="javascript:void(0)">' + statName + '</a></li>';
    breadcrumb.append(ol);
} 
