package com.example.model;

import java.io.Serializable;

public class Task implements Serializable{

	private static final long serialVersionUID = -8980412560768294704L;
	private int taskId;					//任务编号
	private String policeId;			//警员id
	private double longitude;			//经度
	private double latitude;			//纬度
	private String deadLine;			//截止日期
	private String community;			//社区名
	private String room;				//房间名

	public int getTaskId() {
		return taskId;
	}
	public void setTaskId(int taskId) {
		this.taskId = taskId;
	}
	public String getPoliceId() {
		return policeId;
	}
	public void setPoliceId(String policeId) {
		this.policeId = policeId;
	}
	public double getLongitude() {
		return longitude;
	}
	public void setLongitude(double longitude) {
		this.longitude = longitude;
	}
	public double getLatitude() {
		return latitude;
	}
	public void setLatitude(double latitude) {
		this.latitude = latitude;
	}
	public String getDeadLine() {
		return deadLine;
	}
	public void setDeadLine(String deadLine) {
		this.deadLine = deadLine;
	}
	public String getCommunity() {
		return community;
	}
	public void setCommunity(String community) {
		this.community = community;
	}
	public String getRoom() {
		return room;
	}
	public void setRoom(String room) {
		this.room = room;
	}
	public Task(int taskId, String policeId, double longitude, double latitude,
			String deadLine, String community, String room) {
		super();
		this.taskId = taskId;
		this.policeId = policeId;
		this.longitude = longitude;
		this.latitude = latitude;
		this.deadLine = deadLine;
		this.community = community;
		this.room = room;
	}
	
	public Task() {

	}
	
	@Override
	public String toString() {
		return "Task [taskId=" + taskId + ", policeId=" + policeId
				+ ", longitude=" + longitude + ", latitude=" + latitude
				+ ", deadLine=" + deadLine + ", community=" + community
				+ ", room=" + room + "]";
	}
	
}
