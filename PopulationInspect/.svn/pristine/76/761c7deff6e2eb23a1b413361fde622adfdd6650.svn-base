	if($("#room").val().replace(/[^0-9]/ig,"").length == 3){
		data.room_no = "0" + $("#room").val().replace(/[^0-9]/ig,"");
	}
	if($("#room").val().replace(/[^0-9]/ig,"").length == 4){
		data.room_no = $("#room").val().replace(/[^0-9]/ig,"");
	}
  }
	if($('#roomSelect option:selected').val() != "roomSelect"){
