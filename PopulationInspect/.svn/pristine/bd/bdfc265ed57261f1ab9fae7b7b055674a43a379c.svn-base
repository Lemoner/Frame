*{
	margin:0px;
	padding:0px;
}
body,html{
	width:100%;
	height:100%;
	overflow:hidden;
}
#wrapper{
	width:100%;
	height:100%;
	background:rgb(3,96,163);
	text-align:center;
}
#login-img{
	margin-top:60px;
}
#loginbar{
	width:360px;
	height:200px;
	background:rgb(140,206,254);
	border:solid 1px rgb(140,206,254);
	border-radius:3px;
	position:relative;
	top:-340px;
	left:520px;
}
#loginbar ul{
	list-style-type:none;
	padding-top:25px;
}
#loginbar ul li{
	padding:5px;
}
#uname,#upassword{
	height:28px;
	width:220px;
	border-radius:2px;
	border:solid 1px rgb(153,153,153); 
}
span{
	color:rgb(70,70,70);
	font-family:HeiSim;
}
#remember{
	position:relative;
	left:-43px;
	font-size:12px;
	color:rgb(102,102,102);
}
#submit,#reset{
	margin-left:47px;
	width:90px;
	height:30px;
	color:#FFF;
	background:rgb(254,118,0);
	border:solid 1px rgb(254,118,0);
	border-radius:4px;
}
#reset{
	margin-left:35px;
}
#login-p{
	color:#FFF;
	font-family:HeiSim;
	font-size:30px;
	font-weight:bold;
	position:relative;
	top:-600px;
	left:20px;
}